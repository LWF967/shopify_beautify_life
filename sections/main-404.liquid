<style type="text/css">
  .template-404 .title + * {
    margin-top: 1rem;
  }

  @media screen and (min-width: 750px) {
    .template-404 .title + * {
      margin-top: 2rem;
    }
  }
</style>

<div class="template-404 page-width page-margin center">

  <h1 class="title">
    {{ 'templates.404.subtext' | t }} {{ 'templates.404.title' | t }}
  </h1>
  <p>
    {{ 'templates.404.info_text' | t }}
  </p>
  <a href="{{ routes.all_products_collection_url }}" class="button">
    {{ 'general.continue_shopping' | t }}
  </a>
</div>


