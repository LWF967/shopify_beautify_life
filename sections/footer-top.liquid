<footer class="footer color-{{ section.settings.color_scheme }} gradient {% if section.settings.sticky_footer %}my_footer_sticky{% endif %}">
  {%- if section.blocks.size > 0 or section.settings.newsletter_enable or section.settings.show_social -%}
  <div class="my_footer_bgcolor my_footer_margin_top {% if section.settings.border_footer %}my_footer_border{% endif %}">
    <div class="my_footer_top_padding page-width my_footer_bgcolor my_footer_mobile_flex">
      {%- if section.blocks.size > 0 -%}
        {%- liquid
          if section.blocks.size == 9
            assign footer_grid_class = 'grid--3-col-tablet'
          elsif section.blocks.size > 6
            assign footer_grid_class = 'grid--4-col-desktop'
          elsif section.blocks.size > 4
            assign footer_grid_class = 'grid--3-col-tablet'
          endif
        -%}
        <div class="footer__blocks-wrapper grid grid--1-col grid--2-col grid--4-col-tablet my_footer_padding_margin {% if section.settings.accordions_options_on_mobile %}@media (min-width: 0px) and (max-width: 768px){% endif %} {{ footer_grid_class }}">
          {%- for block in section.blocks -%}
            <div class="footer-block grid__item my_footer_mobile_div {% if block.settings.container_width == '1' %}my_footer_item_width_1{% elsif block.settings.container_width == '2' %}my_footer_item_width_2{% elsif block.settings.container_width == '3' %}my_footer_item_width_3{% elsif block.settings.container_width == '4' %}my_footer_item_width_4{% elsif block.settings.container_width == '5' %}my_footer_item_width_5{% elsif block.settings.container_width == '6' %}my_footer_item_width_6{% else %}my_footer_item_width_7{% endif %}
                        {% if block.type == 'link_list' and block.settings.heading != blank %} footer-block--menu{% endif %}" {{ block.shopify_attributes }}>
              {%- case block.type -%}
                {%- when 'text' -%}
                  {%- if block.settings.heading != blank -%}
              	    <h2 class="my_footer_heading my_fotter_mobile_heading">
                      <span>{{- block.settings.heading | escape -}}</span>
                      <div class="my_footer_mobile_show">
                        <i class="my_icon_column"></i>
                        <i class="my_icon_row"></i>
                      </div>
              		</h2>
                  {%- endif -%}
                  <div class="footer-block__details-content rte my_footer_p">
                    {{ block.settings.subtext }}
                  </div>
                {%- when 'text1' -%}
              	  {%- if block.settings.heading != blank -%}
                    <h2 class="my_footer_heading my_fotter_mobile_heading {% if block.settings.image != blank %}my_footer_hide_heading{% endif %}">
                      <span>{{- block.settings.heading | escape -}}</span>
                      <div class="my_footer_mobile_show">
                        <i class="my_icon_column"></i>
                        <i class="my_icon_row"></i>
                      </div>
                    </h2>
                  {%- endif -%}
                  <div class="footer-block__details-content rte my_footer_p">
                        {%- if block.settings.image != blank -%}
                          {%- assign image_size = block.settings.image_width | append: 'x' -%}
                           <img
                               srcset= "{{ block.settings.image | img_url: image_size }}, {{ block.settings.image | img_url: image_size, scale: 2 }} 2x"
                               src="{{ block.settings.image | img_url: '400x' }}"
                               alt="{{ block.settings.image.alt | escape }}"
                               loading="lazy"
                               width="{{ block.settings.custom_logo_width }}"
                               height="{{ block.settings.custom_logo_width }}"
                               style="max-width: min(100%, {{ block.settings.image_width }}px); margin-bottom: 30px;"
                               >
                        {%- endif -%}
                    
                    {{ block.settings.textarea }}
                  </div>
                {%- when 'text2' -%}
                  {%- if block.settings.heading != blank -%}
              	    <h2 class="my_footer_heading my_fotter_mobile_heading">
                      <span>{{- block.settings.heading | escape -}}</span>
                      <div class="my_footer_mobile_show">
                        <i class="my_icon_column"></i>
                        <i class="my_icon_row"></i>
                      </div>
              		</h2>
                  {%- endif -%}
                  <div class="footer-block__details-content rte my_footer_p">
                    <p>{{ block.settings.sub_text }}</p>
                    {%- if  block.settings.newsletter_design == '1' -%}
                      {%- form 'customer', id: 'ContactFooter', class: 'footer__newsletter newsletter-form my_form_margin0' -%}
                        <input type="hidden" name="contact[tags]" value="newsletter">
                        <div class="newsletter-form__field-wrapper my_footer_email_width">
                          <div class="field">
                            <input
                              id="NewsletterForm--{{ section.id }}"
                              type="email"
                              name="contact[email]"
                              class="field__input my_footer_input my_field__input"
                              value="{{ form.email }}"
                              aria-required="true"
                              autocorrect="off"
                              autocapitalize="off"
                              autocomplete="email"
                              {% if form.errors %}
                                autofocus
                                aria-invalid="true"
                                aria-describedby="ContactFooter-error"
                              {% elsif form.posted_successfully? %}
                                aria-describedby="ContactFooter-success"
                              {% endif %}
                              placeholder="{{ 'newsletter.label' | t }}"
                              required
                            >
                            <label class="field__label" for="NewsletterForm--{{ section.id }}">
                              {{ 'newsletter.label' | t }}
                            </label>
                            <button type="submit" class="newsletter-form__button field__button my_footer_button" name="commit" id="Subscribe" aria-label="{{ 'newsletter.button_label' | t }}">
							  Subscribe
                            </button>
                          </div>
                          {%- if form.errors -%}
                            <small class="newsletter-form__message form__message my_footer_displaynone" id="ContactFooter-error">{% render 'icon-error' %}{{ form.errors.translated_fields['email'] | capitalize }} {{ form.errors.messages['email'] }}</small>
                          {%- endif -%}
                        </div>
                        {%- if form.posted_successfully? -%}
                          <h3 class="newsletter-form__message newsletter-form__message--success form__message" id="ContactFooter-success" tabindex="-1" autofocus>{% render 'icon-success' %}{{ 'newsletter.success' | t }}</h3>
                        {%- endif -%}
                      {%- endform -%}
                    {%- elsif block.settings.newsletter_design == '2' -%}
                      {%- form 'customer', id: 'ContactFooter', class: 'footer__newsletter newsletter-form my_form_margin0' -%}
