{%- style -%}
.my_footer_a_hover { text-decoration: none; display: inline-block; transition: all .3s; } .my_footer_a_hover:hover { color: {{ section.settings.hover_link_color }} !important; }
.my_footer_bgcolor { background: {{ section.settings.background_color }}; }
.my_footer_heading { color: {{ section.settings.heading_color }}; font-size: 18px; margin-bottom: 30px; margin-top: 0; font-weight: 700; }
.my_footer_item_width { flex: 0 0 {{ section.blocks.settings.container_width }}; max-width: {{ section.blocks.settings.container_width }}; }
.my_footer_bgcolor.my_footer_margin_top a { color: {{ section.settings.link_color }}; }
.my_footer_p { color: {{ section.settings.text_color }}; }
.my_footer_p a { transition: all .3s; } .my_footer_p a:hover { color: {{ section.settings.hover_link_color }} !important; }
.my_footer_border { border-top: 1px solid {{ section.settings.border_color }};  border-bottom: 1px solid {{ section.settings.border_color }}; }

.my_footer_input { height: 44px; color: {{ section.settings.newsletter_input_color }}; border: 1px solid {{ section.settings.newsletter_border_color }}; }
.my_footer_input:hover { box-shadow: none; }
.my_field__input { box-shadow: none; }
.my_field__input:focus { outline: transparent; box-shadow: none; }
.my_footer_button { color: {{ section.settings.newsletter_button_text_color }}; background: {{ section.settings.newsletter_button_color }}; height: 40px; margin: 2px 2px 1px 0; width: 30%; }
.my_footer_displaynone { display: none; }
{%- endstyle -%}

