<script>
    /*footertop移动端手风琴*/
    var footer_mobile_show = document.getElementsByClassName('my_footer_mobile_show');
	var footer_h2 = document.getElementsByClassName('my_footer_heading my_fotter_mobile_heading');
  
    for (let i = 0; i < footer_mobile_show.length; i++) {
        footer_mobile_show[i].onclick = function () {
            var yuansu = footer_h2[i].nextElementSibling;

            if (yuansu.style.height) {
              // 高度有值
                this.children[0].style.transform = 'rotate(0deg)';
                this.children[0].style.opacity = '1';
                this.children[1].style.transform = 'rotate(90deg)';
                yuansu.style.height = null;

            } else {
              // 高度为0
                this.children[0].style.transform = 'rotate(180deg)';
                this.children[0].style.opacity = '0';
                this.children[1].style.transform = 'rotate(360deg)';
                yuansu.style.height = yuansu.scrollHeight + "px";
            }
        }
    }
</script>